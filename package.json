{
  "name": "nuxt-app",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint:js": "eslint --ext \".ts,.vue\" --ignore-path .gitignore src/",
    "lint:prettier": "prettier --check src/",
    "lint": "npm run lint:js && npm run lint:prettier",
    "lintfix": "prettier --write --list-different src/ && npm run lint:js --fix"
  },
  "devDependencies": {
    "@nuxt/devtools": "latest",
    "@types/node": "^18.17.3",
    "eslint-plugin-nuxt": "^4.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "nuxt": "^3.6.5",
    "nuxt-icon": "^0.5.0",
    "vite-svg-loader": "^4.0.0"
  },
  "dependencies": {
    "@nuxtjs/eslint-config-typescript": "^12.0.0",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "firebase": "^10.2.0",
    "prettier": "^3.0.1",
    "sass": "^1.65.1",
    "typescript": "^5.1.6"
  }
}
